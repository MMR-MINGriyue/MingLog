    "d3-scale": "^4.0.2",
    "d3-hierarchy": "^3.1.2"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/d3-force": "^3.0.9",
    "@types/d3-selection": "^3.0.10",
    "@types/d3-zoom": "^3.0.8",
    "@types/d3-drag": "^3.0.7",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-hierarchy": "^3.1.6",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.3.0",
    "tsup": "^8.0.0",
    "rimraf": "^5.0.5"
