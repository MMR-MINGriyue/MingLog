{
  "name": "minglog-web",
  "version": "0.1.0",
  "description": "MingLog web application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit",
